{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/app/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/app/static/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
